


<div class="container">
  <div class="search">
    <div class="search-bar">
      <%= form_tag apps_path, method: :get do %>
        <%= text_field_tag :search, params[:search], placeholder: "Search" %>
        <%= submit_tag "Search", class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>
